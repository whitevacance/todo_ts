{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@emotion/babel-plugin",
    "@babel/plugin-transform-arrow-functions"
  ],
  "env": {
    "test": {
      "plugins": ["babel-plugin-dynamic-import-node"]
    }
  }
}
